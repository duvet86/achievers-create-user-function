    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@azure/functions": "^4.1.0",
    "@azure/msal-node": "^2.6.0",
    "dotenv": "^16.3.1",
    "module-alias": "^2.2.3",
